# Environment Variables for Cypress Testing Framework
# Copy this file to .env and fill in your actual values

# Development Environment
DEV_ADMIN_PASSWORD=your_dev_admin_password_here
DEV_USER_PASSWORD=your_dev_user_password_here

# Staging Environment
STAGING_ADMIN_PASSWORD=your_staging_admin_password_here
STAGING_USER_PASSWORD=your_staging_user_password_here

# Production Environment
PROD_ADMIN_PASSWORD=your_prod_admin_password_here
PROD_USER_PASSWORD=your_prod_user_password_here

# Local Environment
LOCAL_ADMIN_PASSWORD=your_local_admin_password_here
LOCAL_USER_PASSWORD=your_local_user_password_here

# Optional: Override base URL for testing
BASE_URL=https://example.com